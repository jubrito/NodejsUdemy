<%- include('../includes/head') %>
</head>
<body>
    <%- include('../includes/navigation') %>
    <main>
        <% if (products.length > 0) { %>
            <ul>
                <% products.forEach(cartProducts => { %> 
                    <li>
                        <%= cartProducts.productData.title %> (<%= cartProducts.quantity %>)
                    </li>
                <% }) %> 
            </ul>
        <% } else { %>
            <h1>No products found</h1>
        <% } %>   
    </main>
    <%- include('../includes/end')  %> 